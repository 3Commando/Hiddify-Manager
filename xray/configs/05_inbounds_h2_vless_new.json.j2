{
	"inbounds": [
				{
			"tag": "h2-vless-new",
			//"listen": "@@vless-tcp-new-h2",
			"listen": "/opt/hiddify-server/xray/run/vlesst.sock,666",
			"protocol": "vless",
			"settings": {
			  "clients": [
                {% for u in users %}
				    {"id": "{{ u['uuid'] }}", "email": "{{ u['uuid'] }}@hiddify.com"},
                {% endfor %}
			  ],
			  "decryption": "none"
			},
			"streamSettings": {
			  "network": "h2",
			  "security": "none",
			   "httpSettings": {
         		 "path": "/{{ hconfigs['path_vless'] }}{{ hconfigs['path_tcp'] }}",
        		}
			  
			},
			"sniffing": {
			  "enabled": true,
			  "destOverride": ["http","tls", "quic"]
			}
		  }	  
	  ]
}