{
	"inbounds": [
		{% for domain,port in realitygrpc.items()%}
		{
			"tag":"realityingrpc_{{port}}",
			"listen": "127.0.0.1",
			"listen_port": {{ port }},
			"tcp_fast_open": true,
			"sniff": true,
			"sniff_override_destination": true,
			"domain_strategy": "prefer_ipv4",
			"proxy_protocol":true,
			"type": "vless",
			"users": [
				{% for u in users %}
    				{"name": "{{ u['uuid'] }}@hiddify.com", "uuid": "{{ u['uuid'] }}","flow": ""},
				{% endfor%}
			],
			"transport": {
				"type": "grpc",
				"service_name": "/{{ hconfigs['path_vless'] }}{{ hconfigs['path_grpc'] }}",
				"idle_timeout": "15s",
				"ping_timeout": "15s"
			},
			"tls":{
				"enabled": true,
				"server_name": "{{domain}}",
				"reality": {
					"enabled": true,
					"handshake": {
						"server": "{{hconfigs['servernames'].split(',')[0] or domain}}",
						"server_port": 443
					},
					"private_key": "{{hconfigs['reality_private_key']}}",
					"short_id": [
						"", 
						{%for sid in hconfigs['reality_short_ids'].split(',') %}
						"{{sid}}",
						{%endfor%}
					],
					"max_time_difference": "2h"
				}
			}
		},
		{%endfor %}
	]
}
